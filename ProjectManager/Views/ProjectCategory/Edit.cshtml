@model ProjectManager.Models.ProjectCategory
@{
    ViewBag.Activity = "專案類型";
    ViewBag.Title = "修改專案類型名稱";
}

<form method="post" action="~/ProjectCategory/Edit" class="form-horizontal">
    <div class="form-group">
        <label class="col-md-2 control-label">專案類型名稱</label>
        <div class="col-md-2">
            <input type="hidden" class="form-control" name="ProjectCategoryID" value="@Model.ProjectCategoryID" />
            <input type="text" class="form-control" name="ProjectCategoryName" value="@Model.ProjectCategoryName" />
        </div>
    </div>
    <div class="form-group">
        <div class="col-md-offset-2 col-md-10">
            <button type="submit" class="btn btn-default">修改</button>
        </div>
    </div>
</form>

